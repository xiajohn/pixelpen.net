{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.3.5",
    "fs-extra": "^11.1.1",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-audio-player": "^0.17.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
